Package: dwc.wells
Title: A Package for Condition Predictions for Drinking Water Wells
Version: 0.1.0
Authors@R: 
    c(person(given = "Mathias",
             family = "Riechel",
             role = "aut",
             email = "mathias.riechel@kompetenz-wasser.de",
             comment = c(ORCID = "0000-0002-1191-5877")),
      person(given = "Michael",
             family = "Rustler",
